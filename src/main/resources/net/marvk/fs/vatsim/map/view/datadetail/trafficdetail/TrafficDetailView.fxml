    <contentNode>
        <PlaceholderBox fx:id="trafficHolder" styleClass="container">
            <content>
                <GridPane fx:id="trafficGrid" hgap="10">
                    <columnConstraints>
                        <ColumnConstraints hgrow="NEVER"/>
