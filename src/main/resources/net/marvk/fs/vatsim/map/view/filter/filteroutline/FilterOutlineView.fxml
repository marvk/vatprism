<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import org.kordamp.ikonli.javafx.FontIcon?>
<SplitPane dividerPositions="0.3" styleClass="container" stylesheets="@../filter.css"
           xmlns="http://javafx.com/javafx/11.0.1"
           xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="net.marvk.fs.vatsim.map.view.filter.filteroutline.FilterOutlineView">
    <items>
        <VBox styleClass="main-container">
            <HBox styleClass="button-bar-horizontal">
                <Button onAction="#add" styleClass="icon-button2">
                    <graphic>
                        <FontIcon iconLiteral="mdi2p-plus"/>
                    </graphic>
                </Button>
                <Button onAction="#delete" fx:id="deleteButton" styleClass="icon-button2">
                    <graphic>
                        <FontIcon iconLiteral="mdi2m-minus"/>
                    </graphic>
                </Button>
            </HBox>
            <ListView fx:id="list" VBox.vgrow="ALWAYS"/>
        </VBox>
        <ScrollPane hbarPolicy="NEVER" minWidth="400" fitToWidth="true">
            <fx:include source="../filtereditor/FilterEditor2View.fxml"/>
        </ScrollPane>
    </items>
</SplitPane>
